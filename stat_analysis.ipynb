    "import pandas as pd\n",
    "from scipy.stats import spearmanr\n",
    "\n",
    "top_pred = pd.read_csv(\"top50_docking_scores.csv\")\n",
    "bot_pred = pd.read_csv(\"bottom50_docking_scores.csv\")\n",
    "\n",
    "top_dock = pd.read_csv(\"scores_top50.csv\")\n",
    "bot_dock = pd.read_csv(\"scores_bottom50.csv\")\n",
    "\n",
    "\n",
    "pred = pd.concat([top_pred, bot_pred], ignore_index=True)\n",
    "dock = pd.concat([top_dock, bot_dock], ignore_index=True)\n",
    "\n",
    "df = pred.merge(dock, on=\"rank\")\n",
    "\n",
    "df[\"binding_strength\"] = -df[\"Best_Affinity\"]\n",
    "\n",
    "rho, pval = spearmanr(\n",
    "    df[\"predicted_activity\"],\n",
    "    df[\"binding_strength\"]\n",
    ")\n",
    "\n",
    "print(f\"Spearman rho: {rho:.3f}\")\n",
    "print(f\"p-value: {pval:.6f}\")\n"
   ]
  },
