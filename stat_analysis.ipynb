    "def cliffs_delta(x, y):\n",
    "    x = np.array(x)\n",
    "    y = np.array(y)\n",
    "    gt = np.sum(x[:, None] < y)\n",
    "    lt = np.sum(x[:, None] > y)\n",
    "    return (gt - lt) / (len(x) * len(y))\n",
    "\n",
    "delta = cliffs_delta(top_scores, bottom_scores)\n",
    "print(\"Cliff's Delta:\", delta)\n"
   ]
