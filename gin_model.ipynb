    {
      "cell_type": "code",
      "source": [
        "import torch\n",
        "graphs = torch.load(\"qs_graphs.pt\", weights_only=False)\n",
        "df = pd.read_csv(\"qs_inhibitors_cleaned.csv\")\n",
        "\n",
        "assert len(graphs) == len(df)\n",
        "print(\"Loaded:\", len(graphs))"
      ],
      "metadata": {
