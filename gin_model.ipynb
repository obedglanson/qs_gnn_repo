    {
      "cell_type": "code",
      "source": [
        "def get_murcko_scaffold(smiles):\n",
        "    mol = Chem.MolFromSmiles(smiles)\n",
        "    scaffold = MurckoScaffold.GetScaffoldForMol(mol)\n",
        "    return Chem.MolToSmiles(scaffold)\n",
        "\n",
        "df[\"scaffold\"] = df[\"smiles_canonical\"].apply(get_murcko_scaffold)\n"
      ],
      "metadata": {
        "id": "OplM6xeMFJ7r"
      },
      "execution_count": null,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "def scaffold_split(df, test_fraction=0.2, seed=42):\n",
        "    rng = np.random.default_rng(seed)\n",
        "    scaffolds = df[\"scaffold\"].unique()\n",
        "    rng.shuffle(scaffolds)\n",
        "\n",
        "    test_scaffolds = set()\n",
        "    count = 0\n",
        "\n",
        "    for s in scaffolds:\n",
        "        idxs = df[df[\"scaffold\"] == s].index\n",
        "        test_scaffolds.add(s)\n",
        "        count += len(idxs)\n",
        "        if count / len(df) >= test_fraction:\n",
        "            break\n",
        "\n",
        "    test_idx = df[df[\"scaffold\"].isin(test_scaffolds)].index.tolist()\n",
        "    train_idx = df[~df[\"scaffold\"].isin(test_scaffolds)].index.tolist()\n",
        "\n",
        "    return train_idx, test_idx\n"
      ],
      "metadata": {
        "id": "VBSj_CESFMTS"
      },
      "execution_count": null,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "def make_loaders(graphs, train_idx, test_idx, batch_size=32):\n",
        "    train_graphs = [graphs[i] for i in train_idx]\n",
        "    test_graphs = [graphs[i] for i in test_idx]\n",
        "\n",
        "    return (\n",
        "        DataLoader(train_graphs, batch_size=batch_size, shuffle=True),\n",
        "        DataLoader(test_graphs, batch_size=batch_size, shuffle=False)\n",
        "    )\n"
      ],
      "metadata": {
        "id": "neENIvv8FXIj"
      },
      "execution_count": null,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "from torch_geometric.nn import GINConv, global_mean_pool, BatchNorm\n",
        "\n",
        "class GIN(torch.nn.Module):\n",
        "    def __init__(self, in_channels, hidden_dim=128, dropout=0.3):\n",
        "        super().__init__()\n",
        "\n",
        "        def mlp(in_dim, out_dim):\n",
        "            return torch.nn.Sequential(\n",
        "                torch.nn.Linear(in_dim, out_dim),\n",
        "                torch.nn.ReLU(),\n",
        "                torch.nn.Linear(out_dim, out_dim)\n",
        "            )\n",
        "\n",
        "        self.conv1 = GINConv(mlp(in_channels, hidden_dim), train_eps=True)\n",
        "        self.bn1 = BatchNorm(hidden_dim)\n",
        "\n",
        "        self.conv2 = GINConv(mlp(hidden_dim, hidden_dim), train_eps=True)\n",
        "        self.bn2 = BatchNorm(hidden_dim)\n",
        "\n",
        "        self.dropout = dropout\n",
        "        self.lin = torch.nn.Linear(hidden_dim, 1)\n",
        "\n",
        "    def forward(self, x, edge_index, batch):\n",
        "        x = self.conv1(x, edge_index)\n",
        "        x = self.bn1(x)\n",
        "        x = F.relu(x)\n",
        "        x = F.dropout(x, p=self.dropout, training=self.training)\n",
        "\n",
        "        x = self.conv2(x, edge_index)\n",
        "        x = self.bn2(x)\n",
        "        x = F.relu(x)\n",
        "        x = F.dropout(x, p=self.dropout, training=self.training)\n",
        "\n",
        "        x = global_mean_pool(x, batch)\n",
        "        return self.lin(x).view(-1)\n"
      ],
      "metadata": {
        "id": "GR7EhEfLFZBB"
      },
      "execution_count": null,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "best_val_auc = 0.0\n",
        "best_state = None\n",
        "patience = 15\n",
        "patience_counter = 0\n"
      ],
      "metadata": {
        "id": "piwCWfXwMPfm"
      },
      "execution_count": null,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "def train_epoch(model, loader, optimizer, device, pos_weight):\n",
        "    model.train()\n",
        "    total_loss = 0.0\n",
        "\n",
        "    for data in loader:\n",
        "        data = data.to(device)\n",
        "        optimizer.zero_grad()\n",
        "\n",
        "        logits = model(\n",
        "            data.x,\n",
        "            data.edge_index,\n",
        "            data.batch\n",
        "        )\n",
        "\n",
        "        loss = F.binary_cross_entropy_with_logits(\n",
        "    logits,\n",
        "    data.y.float(),\n",
        "    pos_weight=pos_weight.to(device)\n",
        ")\n",
        "\n",
        "        loss.backward()\n",
        "        optimizer.step()\n",
        "\n",
        "        total_loss += loss.item()\n",
        "\n",
        "    return total_loss / len(loader)\n"
      ],
      "metadata": {
        "id": "Jr0_Cd0QGHz3"
      },
      "execution_count": null,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "def eval_model(model, loader, device):\n",
        "    model.eval()\n",
        "\n",
        "    y_true = []\n",
        "    y_prob = []\n",
        "\n",
        "    with torch.no_grad():\n",
        "        for data in loader:\n",
        "            data = data.to(device)\n",
        "\n",
        "            logits = model(\n",
        "                data.x,\n",
        "                data.edge_index,\n",
        "                data.batch\n",
        "            )\n",
        "\n",
        "            probs = torch.sigmoid(logits)\n",
        "\n",
        "            y_true.extend(data.y.cpu().numpy())\n",
        "            y_prob.extend(probs.cpu().numpy())\n",
        "\n",
        "    y_true = np.array(y_true)\n",
        "    y_prob = np.array(y_prob)\n",
        "\n",
        "    y_pred = (y_prob >= 0.5).astype(int)\n",
        "\n",
        "    return {\n",
        "        \"ROC_AUC\": roc_auc_score(y_true, y_prob),\n",
        "        \"PR_AUC\": average_precision_score(y_true, y_prob),\n",
        "        \"F1\": f1_score(y_true, y_pred)\n",
        "    }\n"
      ],
      "metadata": {
        "id": "BuyPjQ1rGItp"
      },
      "execution_count": null,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "def split_train_val(train_idx, val_fraction=0.2, seed=42):\n",
        "    rng = np.random.default_rng(seed)\n",
        "    rng.shuffle(train_idx)\n",
        "\n",
        "    val_size = int(len(train_idx) * val_fraction)\n",
        "    val_idx = train_idx[:val_size]\n",
        "    new_train_idx = train_idx[val_size:]\n",
        "\n",
        "    return new_train_idx, val_idx\n"
      ],
      "metadata": {
        "id": "tBwWt7T1LrDD"
      },
      "execution_count": null,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "def run_gin_experiment(\n",
        "    graphs,\n",
        "    df,\n",
        "    n_splits=5,\n",
        "    epochs=100,\n",
        "    batch_size=32,\n",
        "    seed_start=42\n",
        "):\n",
        "    all_results = []\n",
        "\n",
        "    device = torch.device(\"cuda\" if torch.cuda.is_available() else \"cpu\")\n",
        "    print(\"Using device:\", device)\n",
        "\n",
        "    for split in range(n_splits):\n",
        "        print(f\"\\n=== Scaffold split {split + 1}/{n_splits} ===\")\n",
        "\n",
        "        best_val_auc_for_split = 0.0\n",
        "        best_state_for_split = None\n",
        "        patience_counter_for_split = 0\n",
        "\n",
        "        train_idx, test_idx = scaffold_split(\n",
        "            df,\n",
        "            seed=seed_start + split\n",
        "        )\n",
        "        new_train_idx, val_idx = split_train_val(\n",
        "            train_idx,\n",
        "            seed=seed_start + split\n",
        "        )\n",
        "        def make_loader(graphs, idxs, batch_size=32, shuffle=False):\n",
        "            subset = [graphs[i] for i in idxs]\n",
        "            return DataLoader(subset, batch_size=batch_size, shuffle=shuffle)\n",
        "\n",
        "        train_loader = make_loader(\n",
        "            graphs, new_train_idx, batch_size=batch_size, shuffle=True\n",
        "        )\n",
        "        val_loader = make_loader(\n",
        "            graphs, val_idx, batch_size=batch_size, shuffle=False\n",
        "        )\n",
        "        test_loader = make_loader(\n",
        "            graphs, test_idx, batch_size=batch_size, shuffle=False\n",
        "        )\n",
        "        train_labels = np.concatenate(\n",
        "            [graphs[i].y.numpy() for i in new_train_idx]\n",
        "        )\n",
        "\n",
        "        pos_weight = torch.tensor(\n",
        "            (len(train_labels) - train_labels.sum()) / train_labels.sum(),\n",
        "            dtype=torch.float,\n",
        "            device=device\n",
        "        )\n",
        "\n",
        "\n",
        "        model = GIN(\n",
        "            in_channels=graphs[0].x.shape[1],\n",
        "            hidden_dim=128\n",
        "        ).to(device)\n",
        "\n",
        "        optimizer = torch.optim.Adam(\n",
        "            model.parameters(),\n",
        "            lr=1e-3,\n",
        "            weight_decay=1e-4\n",
        "        )\n",
        "\n",
        "        for epoch in range(epochs):\n",
        "            loss = train_epoch(\n",
        "                model,\n",
        "                train_loader,\n",
        "                optimizer,\n",
        "                device,\n",
        "                pos_weight\n",
        "            )\n",
        "\n",
        "\n",
        "            val_metrics = eval_model(model, val_loader, device)\n",
        "            val_auc = val_metrics[\"ROC_AUC\"]\n",
        "\n",
        "            if val_auc > best_val_auc_for_split:\n",
        "                best_val_auc_for_split = val_auc\n",
        "                import copy\n",
        "                best_state_for_split = copy.deepcopy(model.state_dict())\n",
        "\n",
        "                patience_counter_for_split = 0\n",
        "            else:\n",
        "                patience_counter_for_split += 1\n",
        "\n",
        "            if patience_counter_for_split >= patience:\n",
        "                print(f\"Early stopping at epoch {epoch+1}\")\n",
        "                break\n",
        "\n",
        "            if (epoch + 1) % 10 == 0:\n",
        "                print(f\"Epoch {epoch+1}, Loss {loss:.4f}, Val ROC-AUC {val_auc:.4f}\")\n",
        "\n",
        "        if best_state_for_split is not None:\n",
        "            model.load_state_dict(best_state_for_split)\n",
        "        else:\n",
        "            print(\"Warning: No best state saved for this split, using model from last epoch.\")\n",
        "\n",
        "        metrics = eval_model(\n",
        "            model,\n",
        "            test_loader,\n",
        "            device\n",
        "        )\n",
        "\n",
        "        print(\"Test Metrics for split:\", metrics)\n",
        "        all_results.append(metrics)\n",
        "\n",
        "    results_df = pd.DataFrame(all_results)\n",
        "\n",
        "    summary_df = results_df.agg([\"mean\", \"std\"])\n",
        "\n",
        "    return results_df, summary_df\n"
      ],
      "metadata": {
        "id": "QQDV-eJCFbhy"
      },
      "execution_count": null,
      "outputs": []
    },
    {
      "cell_type": "code",
